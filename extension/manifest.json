{
  "name": "Detect new Email",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Detect new email and send to local server",
  "action": {},
  "permissions": ["storage", "tabs", "activeTab", "notifications", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://*/*", "https://*/*"],
  "icons": {
    "32": "icon.png",
    "128": "icon.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+M",
        "mac": "Command+M"
      },
      "description": "Open extension"
    }
  },
  "options_page": "options.html"
}
